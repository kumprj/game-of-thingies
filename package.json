  "devDependencies": {
    "concurrently": "^9.2.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.958.0",
    "@aws-sdk/lib-dynamodb": "^3.958.0",
    "@types/canvas-confetti": "^1.9.0",
    "canvas-confetti": "^1.9.4",
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "framer-motion": "^12.23.26",
    "http": "^0.0.1-security",
