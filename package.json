{
  "name": "game-of-thingies",
  "version": "1.0.0",
  "description": "A fun game with friends",
  "main": "server.ts",
  "scripts": {
    "start": "tsx server.ts",
    "dev": "concurrently \"npm run dev:api\" \"npm start --prefix client\"",
    "dev2": "npx concurrently \"tsx server.ts\" \"npm start --prefix client\"",
    "dev:api": "tsx watch server.ts",
    "build": "tsc server.ts --outDir dist --target es2020"
  },
